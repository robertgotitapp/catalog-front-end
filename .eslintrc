{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/destructuring-assignment": 0,
        "react/prop-types": 0,
        "jsx-a11y/label-has-associated-control": 0,
        "jsx-a11y/label-has-for": 0,
        "jsx-quotes": 0,
        "react/prefer-stateless-function": 0,
        "no-unused-vars": 0,
        "no-undef": 0,
        "no-console": 0,
        "import/no-named-as-default": 0
    }
}